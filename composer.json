{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "ext-async": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "akeneo/api-php-client": "dev-use-PSR-for-interoperability",
        "concurrent-php/http": "dev-master",
        "league/csv": "^9.0@dev",
        "nyholm/psr7": "^1.1",
        "php-http/guzzle6-adapter": "^2.0",
        "symfony/console": "4.2.*",
        "symfony/dotenv": "4.2.*",
        "symfony/filesystem": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.2.*",
        "symfony/yaml": "4.2.*",
        "webmozart/assert": "^1.4"
    },
    "require-dev": {
        "donatj/mock-webserver": "^2.0",
        "friends-of-behat/symfony-extension": "^2.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    }
}
